{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest -c jest.config.ts",
    "test:watch": "jest -c jest.config.ts --watch",
    "test:cov": "jest -c jest.config.ts --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest -c jest.config.ts --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset"
  },
  "lint-staged": {
    "*.{ts,js,mjs,cjs}": "eslint --fix",
    "*.{json,md,yml,yaml}": "prettier --write"
  },
  "dependencies": {
    "@nestjs/bullmq": "^11.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-fastify": "^11.1.11",
    "@nestjs/schedule": "^6.1.1",
    "@nestjs/throttler": "^6.5.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@taskforge/contracts": "workspace:*",
    "@taskforge/db-access": "workspace:*",
    "@taskforge/queue-config": "workspace:*",
    "bullmq": "^5.58.5",
    "cron-parser": "^4.9.0",
    "fastify": "5.6.2",
    "ioredis": "^5.9.1",
    "jmespath": "^0.16.0",
    "nestjs-pino": "^4.5.0",
    "nestjs-zod": "^5.1.1",
    "pg": "^8.16.3",
    "pino-http": "^11.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/static": "^9.0.0",
    "@fastify/view": "^11.1.1",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/swagger": "^11.2.4",
    "@nestjs/testing": "^11.0.1",
    "@types/jest": "^30.0.0",
    "@types/jmespath": "^0.15.2",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "handlebars": "^4.7.8",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "prisma": "^7.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
