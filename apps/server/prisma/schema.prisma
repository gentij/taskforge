generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model ApiToken {
  id         String   @id @default(cuid())
  name       String
  tokenHash  String   @unique
  scopes     String[]
  createdAt  DateTime @default(now())
  lastUsedAt DateTime?
  revokedAt  DateTime?
}
